<form action="{{ path('newsletter_create') }}" method="post" >
    {{ form_errors(form) }}
<fieldset>
    <h2>Newsletter de La Paperie</h2>
    <div class="clear">
    {{ form_label(form.email, 'Courriel(*) :') }} 
    <div class="input">
    {{ form_errors(form.email) }}
    {{ form_widget(form.email) }}
    </div>
    </div>
    <div class="clear">
    {{ form_label(form.firstname, 'Pr√©nom(*) :') }}
    <div class="input">
    {{ form_errors(form.firstname) }}
    {{ form_widget(form.firstname) }}
    </div>
    </div>
    <div class="clear">
    {{ form_label(form.lastname, 'Nom(*) :') }}
    <div class="input">
    {{ form_errors(form.lastname) }}
    {{ form_widget(form.lastname) }}
    </div>
    </div>
    <div class="clear">
    {{ form_label(form.address, 'Adresse :') }}
    <div class="input">
    {{ form_errors(form.address) }}
    {{ form_widget(form.address) }}
    </div>
    </div>
    <div class="clear">
    {{ form_label(form.ville, 'Ville :') }}
    <div class="input">
    {{ form_errors(form.ville) }}
    {{ form_widget(form.ville) }}
    </div>
    </div>
    <div class="clear">
    {{ form_label(form.codePostal, 'Code Postal :') }}
    <div class="input">
    {{ form_errors(form.codePostal) }}
    {{ form_widget(form.codePostal) }}
    </div>
    </div>
    {{ form_rest(form) }}
</fieldset>
<p>
        <button type="submit" class="">S'inscrire</button>
</p>
</form>
