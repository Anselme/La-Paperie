{% if actionsculturelles %}
<ul class="subnav">
{% for action in actionsculturelles %}
<li class="{{ action.id == subcurrent ? 'active' : ''}}" >
<a href="{{ path('actionculturelle_detail', {'id':  action.id  }) }}">{{ action.title }}</a>
</li>
{% endfor %}
</ul>
{% endif %}

{% if archives %}
<ul class="subnav">
{% for archive in archives %}
<li class="" >
<a href="{{ path('actionculturelle') }}">{{ archive.year }}</a>
</li>
{% endfor %}
</ul>
{% endif %}
